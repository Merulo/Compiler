DECLARE
  a; b; c; d; e; f; g; h; i; j; k; l; m; n; o; p;
IN
  a := 10;
  b := 20;
  e := 100;
  f := 200;
  WRITE a;
  IF a < b THEN
    WRITE e;
    d := 40;
    c := 30;
    WRITE f;
    g:=10;
    h:=10;
    i:=10;
    j:=10;
    k:=10;
    l:=10;
    m:=10;
    n:=10;
    o:=10;
    p:=10;
  ENDIF
  WRITE c;
  WRITE d;

END