DECLARE
  array(0:100);
IN
  array(0) := 1;
  array(1) := 2;
  array(2) := 3 + 4;
  array(3) := array(0) + 5;
  array(4) := 6 + array(0);
  array(5) := array(0) + array(1);
  array(6) := array(1) + array(0);
  array(7) := array(0) + array(1);
  array(7) := array(7) + array(0);
  array(8) := array(0) + array(1);
  array(8) := array(0) + array(8);

  WRITE array(0);
  WRITE array(1);
  WRITE array(2);
  WRITE array(3);
  WRITE array(4);
  WRITE array(5);
  WRITE array(6);
  WRITE array(7);
  WRITE array(8);
END