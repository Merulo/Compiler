DECLARE
	a; b; c; tab(0:100000); d; e; f; tabb(1000:10000); g; h; i; j; k; l; m; n; o; p; z;
IN
	d := 10;
	a := 4;
	b := 3;
	WHILE d > 0 DO
		WHILE a> 0 DO
			a:= a- 1;
			WRITE a;
		ENDWHILE
		WHILE b> 0 DO
			b:= b- 1;
			WRITE b;
		ENDWHILE	
		d := d - 1;
		WRITE d;
		a := 4;
		b := 3;
	ENDWHILE
END 