DECLARE
  a; b; c; d; tab_a(0:1); tab_b(0:1); tab_c(0:1);
IN
  tab_a(0) := 1;
  tab_b(0) := 2;
  tab_c(0) := 3;
  tab_a(1) := 4;
  tab_b(1) := 5;
  tab_c(1) := 6;  

  a := 0;
  WRITE tab_a(0);
  WRITE tab_b(0);
  WRITE tab_c(0);
  a := a + 1;
  WRITE tab_a(1);
  WRITE tab_b(1);
  WRITE tab_c(1);
END
